{"ast":null,"code":"import { jsxs as _jsxs } from \"react/jsx-runtime\";\nimport { jsx as _jsx } from \"react/jsx-runtime\";\nimport React from 'react';\nimport _ from 'lodash';\nimport { getData, withPrefix, markdownify } from '../utils';\nexport default class TeamSection extends React.Component {\n  renderTeamMember(teamMemberRef, index, data) {\n    const teamMember = getData(data, teamMemberRef);\n\n    if (!teamMember) {\n      return null;\n    }\n\n    const photo = _.get(teamMember, 'photo');\n\n    const photoAlt = _.get(teamMember, 'photo_alt', '');\n\n    const firstName = _.get(teamMember, 'first_name', '');\n\n    const lastName = _.get(teamMember, 'last_name', '');\n\n    const name = _.trim(`${firstName} ${lastName}`);\n\n    const bio = _.get(teamMember, 'bio');\n\n    return /*#__PURE__*/_jsx(\"div\", {\n      className: \"cell\",\n      children: /*#__PURE__*/_jsxs(\"div\", {\n        className: \"card team-member\",\n        children: [photo && /*#__PURE__*/_jsx(\"figure\", {\n          className: \"card__media card__media--bottom\",\n          children: /*#__PURE__*/_jsx(\"img\", {\n            src: withPrefix(photo),\n            alt: photoAlt\n          })\n        }), /*#__PURE__*/_jsxs(\"div\", {\n          className: \"card__body\",\n          children: [/*#__PURE__*/_jsx(\"header\", {\n            className: \"card__header\",\n            children: /*#__PURE__*/_jsx(\"h3\", {\n              className: \"h4 card__title\",\n              children: name\n            })\n          }), bio && /*#__PURE__*/_jsx(\"div\", {\n            className: \"card__copy\",\n            children: markdownify(bio)\n          })]\n        })]\n      })\n    }, index);\n  }\n\n  render() {\n    const data = _.get(this.props, 'data');\n\n    const section = _.get(this.props, 'section');\n\n    const title = _.get(section, 'title');\n\n    const team = _.get(section, 'team');\n\n    return /*#__PURE__*/_jsxs(\"section\", {\n      className: \"section section--team\",\n      children: [title && /*#__PURE__*/_jsx(\"div\", {\n        className: \"container container--md align-center\",\n        children: /*#__PURE__*/_jsx(\"h2\", {\n          className: \"section__title\",\n          children: title\n        })\n      }), !_.isEmpty(team) && /*#__PURE__*/_jsx(\"div\", {\n        className: \"container container--lg\",\n        children: /*#__PURE__*/_jsx(\"div\", {\n          className: \"flex flex--col-3\",\n          children: _.map(team, (teamMemberRef, index) => this.renderTeamMember(teamMemberRef, index, data))\n        })\n      })]\n    });\n  }\n\n}","map":null,"metadata":{},"sourceType":"module"}